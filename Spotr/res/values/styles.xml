    <!-- Title Bar -->
    <style name="CustomWindowTitleBackground">
        <item name="android:background">@color/aluminum6</item>
    </style>
 
    <style name="CustomTheme" parent="android:Theme">
        <item name="android:windowTitleSize">50dip</item>
        <item name="android:windowTitleBackgroundStyle">@style/CustomWindowTitleBackground</item>
    </style>
    <!-- End Title Bar -->
    
    <!-- TabWidget -->
    <style name="TabBackground" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/tab_selector</item>
    </style>
    
    <style name="TabText" >
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">8dp</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">@color/aluminum1</item>
    </style>
    <!-- End TabWidget -->
        
    <!-- Main Menu -->
    <style name="MainMenuButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:drawablePadding">6dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/aluminum6</item>
        <item name="android:background">@null</item>
    </style>
    
    <style name="NotificationHandle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:background">@color/aluminum4</item>
    </style>
    <!-- End Main Menu -->

    <!-- Spots: Missions tab -->    
    <style name="MissionsList">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:dividerHeight">1dp</item>
        <item name="android:footerDividersEnabled">false</item>
        <item name="android:scrollbars">none</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:drawSelectorOnTop">false</item>
    </style>
    
    <style name="MissionRow">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">7dp</item>
    </style>
    
    <style name="MissionIcon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>
		    
    <style name="MissionName">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/aluminum5</item>
    </style>
    
    <style name="MissionPointValue">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_horizontal|right</item>
        <item name="android:textSize">40sp</item>
        <item name="android:textStyle">bold|italic</item>
        <item name="android:textColor">@color/aluminum5</item>
    </style>
    
    <style name="MissionPointValueAbbrev">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/aluminum5</item>
    </style>
    <!-- End Spots: Missions tab -->
    
    <!-- Spots: Missions Tab: Open Treasure Button-->
    <style name="TreasureButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textSize">20dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">2</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
    </style>
    
      <!-- Spots: Missions Tab: Open Treasure Button-->
    <style name="InfoButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">0dp</item>
        <item name="android:textSize">10dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">2</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">2dp</item>
    </style>
    
    <!-- Comment Button -->
    <style name="CommentButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">0dp</item>
        <item name="android:textSize">8dp</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">2dp</item>
    </style>
    
    <!-- Like Button -->
    <style name="LikeButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">0dp</item>
        <item name="android:textSize">8dp</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">2dp</item>
    </style>  
    
    <!-- Me: Leaderboard List -->
    <style name="RankListing">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:listSelector">@android:color/transparent</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:divider">@android:color/transparent</item>
    </style>
    
    <!-- Me: Leaderboard Row -->
    <style name="LeaderboardRow">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:orientation">horizontal</item>
    </style>
    
    <!-- Me: Leaderboard Header -->
    <style name="LeaderboardRow.Header">
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:background">@color/aluminum1</item>
    </style>
            
    <!-- Me: Leaderboard Rank -->
    <style name="LeaderboardRank">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item> 
        <item name="android:textColor">@color/aluminum6</item>
    </style>
    
    <!-- Me: Leaderboard Username -->
    <style name="LeaderboardUsername">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">5.5</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:singleLine">true</item> 
<!--         <item name="android:ellipsize">marquee</item> -->
<!--         <item name="android:marqueeRepeatLimit">marquee_forever</item> -->
<!--         <item name="android:focusable">true</item> -->
<!--         <item name="android:focusableInTouchMode">true</item>  -->
<!--         <item name="android:scrollHorizontally">true</item> -->
        <item name="android:textColor">@color/aluminum6</item>  
    </style>    
    
    <!-- Me: Leaderboard Stat Values -->
    <style name="LeaderboardStats">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1.5</item>
        <item name="android:gravity">right|center_vertical</item>
        <item name="android:singleLine">true</item> 
        <item name="android:textColor">@color/aluminum6</item>
    </style>
    
    <!-- Me: Leaderboard Points -->
    <style name="LeaderboardStats.Points">
        <item name="android:layout_weight">2</item>
    </style>
    
    <style name="LeaderboardRank.Header">
        <item name="android:textStyle">bold</item>
    </style>
     
    <style name="LeaderboardUsername.Header">
        <item name="android:textStyle">bold</item>
    </style>
    
    <style name="LeaderboardStats.Header">
        <item name="android:textStyle">bold</item>
    </style>
    
    <style name="LeaderboardStats.Points.Header">
        <item name="android:textStyle">bold</item>
    </style>
    
</resources>